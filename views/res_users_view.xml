<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="res_users_form_view_gt" model="ir.ui.view">
            <field name="name">res.users.form.view.pos_gt</field>
            <field name="model">res.users</field>
            <field name="inherit_id" ref="base.view_users_form"/>
            <field name="arch" type="xml">
                <data>
                    <notebook position="inside">
                        <page string="Tienda">
                            <group>
                                <field name="default_pos_id"/>
                                <field name="default_location_id"/>
                                <field name="default_analytic_account_id"/>
                            </group>
                        </page>
                    </notebook>
                </data>
            </field>
        </record>

    </data>
</odoo>
